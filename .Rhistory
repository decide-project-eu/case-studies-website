require(openssl)
# Function to apply SHA-256 hashing
sha256_hash <- function(data) {
openssl::sha256(data)
}
dplyr::mutate(
Filenumber= sha256_hash(as.character(Filenumber)),
Samplenumber= sha256_hash(as.character(Samplenumber))
)
